
<div id="<%= "admin-image-#{image.id}" %>" class="col-md-2">
  <%= link_to image_tag( image.image.url(:medium)), image.image.url, target: '_blank', class: 'thumbnail' %>
  <%= link_to '<i class="glyphicon glyphicon-trash"></i>'.html_safe, admin_image_path(image), method: :delete, remote: true, class: 'close'  %>
</div>