= form_for(@admin_order_history) do |f|
  - if @admin_order_history.errors.any?
    #error_explanation
      %h2
        = pluralize(@admin_order_history.errors.count, "error")
        prohibited this admin_order_history from being saved:
      %ul
        - @admin_order_history.errors.full_messages.each do |msg|
          %li= msg
  .actions
    = f.submit
